{
    "name": "awesome-components",
    "version": "0.1.1",
    "description": "A modern, lightweight React component library with TypeScript support",
    "main": "./dist/awesome-components.cjs.js",
    "module": "./dist/awesome-components.esm.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/awesome-components.esm.js",
            "require": "./dist/awesome-components.cjs.js",
            "types": "./dist/index.d.ts"
        },
        "./styles.css": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "react",
        "components",
        "ui",
        "typescript",
        "component-library"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/awesome-components.git"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.6.10",
        "@storybook/addon-interactions": "^7.6.10",
        "@storybook/addon-links": "^7.6.10",
        "@storybook/blocks": "^7.6.10",
        "@storybook/react": "^7.6.10",
        "@storybook/react-vite": "^7.6.10",
        "@storybook/testing-library": "^0.2.2",
        "@testing-library/react": "^14.1.2",
        "@testing-library/jest-dom": "^6.2.0",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-storybook": "^0.6.15",
        "jsdom": "^24.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "storybook": "^7.6.10",
        "typescript": "^5.3.3",
        "vite": "^5.0.11",
        "vite-plugin-dts": "^3.7.1",
        "vitest": "^1.2.1"
    }
}
